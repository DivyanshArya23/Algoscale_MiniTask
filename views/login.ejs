<div class="container col-md-4 col-sm-10">
    <div class="row">
        <h1 class="text-center mx-auto">Log In</h1>
    </div>
    <form action="/login" method="POST">
        <div class="form-group">
            <input class="form-control" type="text" placeholder="Email" name="username">
        </div>
        <div class="form-group">
            <input class="form-control" type="password" placeholder="password" name="password">
            <small id="PasswordHelp" class="form-text text-muted">Never share your password with anyone else.</small>
        </div>
        <button type="submit" class="w-100 btn btn-primary">Submit</button>
    </form>
</div>

